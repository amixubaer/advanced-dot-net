@model List <ProductForm.Models.Entity.Order>
    @{
        ViewBag.Title = "Order";
    }

    <h2>Order</h2>

    <table class="table table-striped table-hover">
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Price</td>
            <td>Quantity</td>
            <td>Description</td>
        </tr>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.Id</td>
                <td>@p.Name</td>
                <td>@p.Price</td>
                <td>@p.Quantity</td>
                <td>@p.Description</td>

            </tr>
        }

    </table>
